package com.springIoc;

import org.springframework.stereotype.Component;

@Component
public class Person {
	
	private int id;
	private String name;
	
	public Person(int id, String name) {
		this.id = id;
		this.name = name;
	}
	
	public Person getPerson() {
		return new Person(1,"Somanath");
	}
	
	

}
